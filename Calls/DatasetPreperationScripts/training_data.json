[
    {
        "input_text": "Get data center from SPOG with data center id 6",
        "api_request": {
            "endpoint": "https://spogbe.wxccqa.com/dc",
            "method": "GET",
            "params": {
                "dc_id": "6"
            }
        }
    },
    {
        "input_text": "Get all teams in SPOG",
        "api_request": {
            "endpoint": "https://spogbe.wxccqa.com/teams/",
            "method": "GET"
        }
    },
    {
        "input_text_template": "Get detail of team {team_id} from SPOG",
        "endpoint_template": "https://spogbe.wxccqa.com/teams/{team_id}",
        "type": "template1",
        "method": "GET",
        "variables": {
            "team_id": [10, 30, 60, 90]
        }
    },
    {
        "input_text_template": "Get run status of last {days} days from spog across teams",
        "endpoint_template": "https://spogbe.wxccqa.com/runs/status/all/{days}",
        "type": "template1",
        "method": "GET",
        "variables": {
            "days": [10, 30, 60, 90]
        }
    },
    {
        "input_text_template": "Get runs of team {team_id} from SPOG",
        "endpoint_template": "https://spogbe.wxccqa.com/runs/{team_id}",
        "type": "template1",
        "method": "GET",
        "variables": {
            "team_id": [10, 16, 7, 8]
        }
    },
    {
        "input_text": "Get reservations for team id {team_id} from SPOG",
        "api_request": {
            "endpoint": "https://spogbe.wxccqa.com/reserve/",
            "method": "GET",
            "params": {
                "team_id": "16"
            }
        }
    },
    {
        "input_text_template": "Create a user with email {email} in team with ID {teamId}",
        "type": "template2",
        "api_request": {
            "endpoint": "https://spogbe.wxccqa.com/users/",
            "method": "POST",
            "headers": {
                "accept": "application/json",
                "Content-Type": "application/json"
            },
            "data": {
                "email": "{email}",
                "teamId": "{teamId}"
            }
        },
        "variables": {
            "email": ["pnarah@maildummy.com", "pnarah1@maildummy.com", "pnarah2@maildummy.com", "pnarah3@maildummy.com"],
            "teamId": [10, 16, 7, 8]
        }
    }
]
